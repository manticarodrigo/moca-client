{
  "extends": [
    "tslint-config-airbnb",
    "tslint-react"
  ],
  "rules": {
    "import-name": [false],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case"
    ],
    "align": [
      true,
      "parameters",
      "statements",
      "members",
      "elements"
    ],
    "jsx-no-multiline-js": false,
    "jsx-boolean-value": false
  }
}